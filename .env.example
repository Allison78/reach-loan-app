# Vercel Edge Config
# Full Edge Config URL (managed by Vercel CLI)
EDGE_CONFIG="https://edge-config.vercel.com/YOUR_CONFIG_ID?token=YOUR_READ_TOKEN"

# Edge Config details for API routes
EDGE_CONFIG_ID=your_edge_config_id
EDGE_CONFIG_READ_TOKEN=your_read_token
EDGE_CONFIG_ITEM_KEY=loanApplicants

# Vercel API Token (for updating Edge Config)
# Get this from: https://vercel.com/account/tokens
VERCEL_API_TOKEN=your_vercel_api_token

# Vercel OIDC Token (managed by Vercel CLI)
VERCEL_OIDC_TOKEN=your_oidc_token
